{
    "name": "youngones/lightspeed",
    "description": "Simple gRPC alternative for existing projects.",
    "license": "MIT",
    "authors": [
        {
            "name": "Wesley Peeters",
            "email": "wesley@youngones.works",
            "homepage": "https://www.youngones.works"
        }
    ],
    "homepage": "https://github.com/youngones/lightspeed",
    "keywords": ["Laravel", "Lightspeed"],
    "require": {
        "php": "^7.4|^8.0",
        "2tvenom/cborencode": "^1.0",
        "guzzlehttp/psr7": "^1.7",
        "illuminate/support": "~7|~8",
        "laminas/laminas-text": "^2.8",
        "react/socket": "^1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "YoungOnes\\Lightspeed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YoungOnes\\Lightspeed\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YoungOnes\\Lightspeed\\LightspeedServiceProvider"
            ],
            "aliases": {
                "Lightspeed": "YoungOnes\\Lightspeed\\Facades\\Lightspeed"
            }
        }
    }
}
